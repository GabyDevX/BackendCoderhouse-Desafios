<h2 style='color:crimson;'>
  Historial
</h2>

{{#if ifExpression}}
  <div class='table-responsive'>
    <table class='table table-dark'>
      <tr style='color: yellow;'>
        <th>
          titulo
        </th>

        <th>
          Precio
        </th>

        <th>
          Imagen
        </th>
      </tr>
      {{#each productos}}
        <tr>
          <td>
            {{this.titulo}}
          </td>
          <td>
            {{this.precio}}
          </td>
          <td>
            <img
              style="width: 2rem; height: 'auto'"
              src={{this.imagen}}
              alt=''
            />
          </td>
        </tr>
      {{/each}}
    </table>
  </div>
{{else}}
  <h3 class='alert alert-danger'>
    no se encontraron datos
  </h3>
{{/if}}